{
  "name": "map-tile-adapter",
  "version": "0.0.1",
  "description": "Reproject map tiles on the fly with maplibre, leaflet, or vanilla js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "DOTENV_CONFIG_PATH=./.env.local NODE_OPTIONS='-r dotenv/config' rollup -c rollup.config.dev.js --watch",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "cypress:open": "cypress open",
    "clean": "rm -rf dist",
    "build": "npm run clean && DOTENV_CONFIG_PATH=./.env.prod NODE_OPTIONS='-r dotenv/config' rollup -c rollup.config.prod.js"
  },
  "author": "Brian Swedberg",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.45",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/proj4": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-jest": "^29.1.2",
    "cypress": "^10.10.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.0.1",
    "maplibre-gl": "^2.4.0",
    "rollup": "^2.79.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.8.2"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "proj4": "^2.8.0",
    "tilebelt-wgs84": "^1.0.3"
  }
}
